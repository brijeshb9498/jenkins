pipeline {
  agent any
  stages {
    stage('Clone Repository') {
      steps {
        sh ''' git clone 
      }
    }
    stage('Perform Git Operation') {
      steps {
        sh 'git checkout -b master'
        sh ''' git add --all && git commit -m 'new branch master' && git push '''
        sh '' 
      }
    }
  }
}
